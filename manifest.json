{
    "manifest_version": 3,
    "name": "Error Messages",
    "version": "1.0",
    "description": "Detects 404 pages turns them into a canvas.",
    "permissions": [
      "webNavigation",
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }