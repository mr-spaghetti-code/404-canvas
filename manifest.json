{
  "manifest_version": 3,
  "name": "Error Messages",
  "version": "1.1",
  "description": "Detects 404 pages and turns them into a canvas.",
  "permissions": [
    "webNavigation",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}